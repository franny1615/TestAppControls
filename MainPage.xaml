<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:TestAppControls"
             x:DataType="local:MainPage"
             x:Class="TestAppControls.MainPage">
    <VerticalStackLayout Padding="30">
        <local:LabeledEntry
            x:Name="LabelEntry"
            Placeholder="Number"
            Keyboard="Numeric"/>
        <Entry/>
        <Button Text="Tap Me" Clicked="Button_Clicked"/>
    </VerticalStackLayout>
</ContentPage>

